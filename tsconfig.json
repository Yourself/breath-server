{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "faker/*": [
        "./@faker-js/faker/*"
      ],
      "faker": [
        "./@faker-js/faker"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "jest.config.js"
  ]
}